#parse("common/edit/head.vm")

<tr>
	<td width="20%;">配置名称：</td>
	<td colspan="3">#if ($!{editType} == "add" || $!{editType} == "edit") 
		<input type="text" id="name" name="name"  style="width:100%;"
			class="required" maxlength="50" #if($!{editType} == "edit") value="$!{model.name}" #end/>
		#else 
			$!{model.name}
		#end
	</td>
</tr>
<tr>
	<td>配置键：</td>
	<td colspan="3">
		#if($!{editType} == "add" || $!{editType} == "edit") 
		<textarea id="key" name="key" style="width:100%;height:50px;" class="required" 
			maxlength="256"> #if($!{editType} == "edit")$!{model.key}#end</textarea>
		#elseif($!{editType} == "view")
			$!{model.key}
		#end   			
	</td>
</tr>
<tr>
	<td>配置值：</td>
	<td colspan="3">
		#if($!{editType} == "add" || $!{editType} == "edit") 
		<textarea id="value" name="value" style="width:100%;height:50px;" class="required" 
			maxlength="256"> #if($!{editType} == "edit")$!{model.value}#end</textarea>
		#elseif($!{editType} == "view")
			$!{model.value}
		#end   			
	</td>
</tr>
#parse("common/edit/append-fields.vm")
	
#parse("common/edit/bottom.vm")
